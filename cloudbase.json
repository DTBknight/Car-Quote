{
  "envId": "your-env-id",
  "functionRoot": "./cloudfunctions",
  "functions": [
    {
      "name": "generate-contract",
      "runtime": "Python3.7",
      "memorySize": 256,
      "timeout": 60,
      "triggers": [
        {
          "name": "generate-contract",
          "type": "http",
          "config": {
            "methods": ["GET", "POST", "OPTIONS"]
          }
        }
      ]
    }
  ],
  "hosting": {
    "public": "./",
    "ignore": [
      "node_modules/**",
      "cloudfunctions/**",
      ".git/**",
      "*.md",
      "test-*.html"
    ]
  }
} 