{
  "envId": "car-quote-env",
  "functionRoot": "./functions",
  "functions": [
    {
      "name": "api",
      "runtime": "Python3.9",
      "memory": 512,
      "timeout": 60,
      "triggers": [
        {
          "name": "apiTrigger",
          "type": "http",
          "config": "http://api.car-quote.com"
        }
      ],
      "installDependency": true,
      "ignore": [
        "node_modules/**/*",
        ".git/**/*",
        "*.log"
      ]
    }
  ],
  "staticRoot": "./public",
  "static": {
    "hosting": {
      "public": "./public",
      "ignore": [
        "node_modules/**/*",
        ".git/**/*",
        "*.log",
        "cloudbase-deployment/**/*"
      ]
    }
  },
  "framework": {
    "name": "car-quote-app",
    "plugins": {
      "client": {
        "use": "@cloudbase/framework-plugin-website",
        "inputs": {
          "buildCommand": "echo 'Static site - no build required'",
          "outputPath": ".",
          "cloudPath": "/"
        }
      },
      "server": {
        "use": "@cloudbase/framework-plugin-function",
        "inputs": {
          "functionRootPath": "./functions",
          "functions": [
            {
              "name": "api",
              "runtime": "Python3.9",
              "memory": 512,
              "timeout": 60
            }
          ]
        }
      }
    }
  }
} 